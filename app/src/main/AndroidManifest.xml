<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.henrylee.hackdavis2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TitleScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".StudentScreen" />
        <activity
            android:name=".testTabbedActivity"
            android:label="@string/title_activity_test_tabbed"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".TeacherScreen" />
        <activity
            android:name=".StudentQuizScreen"
            android:label="@string/title_activity_student_quiz_screen"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".TeacherCreateQuiz"
            android:label="@string/title_activity_teacher_create_quiz"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".QuestionCreateFragment"
            android:label="QuestionCreate"
            android:theme="@style/AppTheme.NoActionBar" />

        <service
            android:name=".TeacherNotificationService"
            android:enabled="true"
            android:exported="true"></service>
    </application>

</manifest>